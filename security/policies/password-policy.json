{
  "_description": "Password security policy - Enforces strong password requirements",
  "_purpose": "Protects user accounts from unauthorized access",
  "minLength": 12,
  "maxLength": 128,
  "requireUppercase": true,
  "requireLowercase": true,
  "requireNumbers": true,
  "requireSpecialChars": true,
  "specialChars": "!@#$%^&*()_+-=[]{}|;:,.<>?",
  "preventCommonPasswords": true,
  "preventUserInfo": true,
  "preventReuse": 5,
  "expiryDays": 90,
  "warningDays": 14,
  "lockoutAttempts": 5,
  "lockoutDuration": 900
}
